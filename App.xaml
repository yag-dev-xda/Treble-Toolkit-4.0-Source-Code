<Application x:Class="TrebleToolkit4.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TrebleToolkit4"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="lblDesign" TargetType="Label">
            <Setter Property="FontFamily" Value="fonts//Treble Toolkit;component/Fonts/#Bahnschrift.TTF #/Treble Toolkit;component/Fonts/#Bahnschrift"/>
        </Style>
    </Application.Resources>
</Application>
